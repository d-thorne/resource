          <argument>
            <kind>1</kind>
            <string>///rocket jump works 
//Get the player's input
key_right = keyboard_check(ord('D'));
key_left = -keyboard_check(ord('A'));
key_jump = keyboard_check_pressed(vk_space);

//React to inputs
move = gamepad_axis_value(0, gp_axislh);
hsp = round(move * movespeed);
if (vsp &lt; 10) vsp += grav;



 if(place_meeting(x,y+1,obj_wall)&amp;&amp;aim_mode)
    {
        hsp=0;
    }
    
if (place_meeting(x,y+1,obj_wall)&amp;&amp;!aim_mode)
{
    vsp =round(gamepad_axis_value(0, gp_axislv)) * jumpspeed
    hsp = round(move * movespeed);
    in_air=false;
    
}

if (global.jump)
{
    vsp=lengthdir_y(jumpspeed,-global.aim)*2
    hsp=lengthdir_x(jumpspeed,-global.aim)
    h_in_air_b=lengthdir_x(jumpspeed,-global.aim)
    global.jump=false;
    in_air=true
    
}


if(in_air&amp;&amp;gamepad_button_check_pressed(0, gp_face1)||gamepad_button_check_pressed(0, gp_face2)||gamepad_button_check_pressed(0, gp_face3)&amp;&amp;!place_meeting(x,y+1,obj_wall)) h_in_air = -h_in_air_b;
if(gamepad_button_check_pressed(0, gp_face1)||gamepad_button_check_pressed(0, gp_face2)||gamepad_button_check_pressed(0, gp_face3)&amp;&amp;!place_meeting(x,y+1,obj_wall)&amp;&amp;!in_air) h_in_air=hsp;

if(!place_meeting(x,y+1,obj_wall)&amp;&amp;in_air&amp;&amp;aim_mode)
{
    hsp=-h_in_air_b;
}
else if(!place_meeting(x,y+1,obj_wall)&amp;&amp;aim_mode)
{
    hsp=h_in_air;
}
if(gamepad_button_check_pressed(0,gp_face4)&amp;&amp;global.rocket&gt;30)
{
rocket_start=true;
instance_create(x,y,obj_sword);
aim_mode=false;
}
if(gamepad_button_check(0,gp_face4)&amp;&amp;rocket_start&amp;&amp;global.rocket&gt;0&amp;&amp;(abs(gamepad_axis_value(0,gp_axislh))&gt;.25||abs(gamepad_axis_value(0,gp_axislv))&gt;.25))
{
 vsp=lengthdir_y(rocket_speed,global.aim)
 hsp=lengthdir_x(rocket_speed,global.aim)
 if(global.shake&lt;4)
 {
    global.shake=4
    obj_cam.alarm[0] = global.shake_time;
 }
 global.rocket-=1;

}
else if(global.rocket&lt;=global.rockmax)
{
global.rocket+=2/3;
if(global.rocket&lt;30)
    {
    rocket_start=false;
    }
}


//Horizontal Collision
if (place_meeting(x+hsp,y,obj_wall))
{
    while(!place_meeting(x+sign(hsp),y,obj_wall))
    {
        x += sign(hsp);
    }
    h_in_air = 0;
    h_in_air_b = 0;
    hsp = 0;
    in_air=false
}
x += hsp;

//Vertical Collision
if (place_meeting(x,y+vsp,obj_wall))
{
    while(!place_meeting(x,y+sign(vsp),obj_wall))
    {
        y += sign(vsp);
    }
    vsp = 0;
}
y += vsp;

